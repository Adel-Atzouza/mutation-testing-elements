{
  "name": "mutation-testing-elements",
  "version": "0.0.6",
  "description": "A suite of web components for a mutation testing report.",
  "main": "dist/mutation-test-elements.js",
  "scripts": {
    "test": "mocha",
    "start": "webpack-dev-server --open",
    "prebuild": "rimraf dist",
    "build": "webpack --configFile webpack.config.js --mode production",
    "pretest": "npm run build",
    "preversion": "npm t",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "postversion": "npm publish && git push && git push --tags",
    "release:patch": "npm version patch -m \"chore(release): %s\"",
    "release:minor": "npm version minor -m \"chore(release): %s\"",
    "release:major": "npm version major -m \"chore(release): %s\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stryker-mutator/mutation-testing-elements.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/stryker-mutator/mutation-testing-elements/issues"
  },
  "homepage": "https://github.com/stryker-mutator/mutation-testing-elements#readme",
  "devDependencies": {
    "ajv": "^6.5.2",
    "bootstrap": "^4.3.1",
    "chai": "^4.1.2",
    "conventional-changelog-cli": "^2.0.12",
    "css-loader": "^2.1.0",
    "lit-element": "^2.0.1",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.3",
    "terser-webpack-plugin": "^1.2.2",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3",
    "webpack": "^4.29.4",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "highlight.js": "^9.14.2",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0"
  }
}
